{
  "name": "myhealthprices",
  "version": "1.1.0",
  "description": "Web app for displaying health-related prices on myhealthprices.com",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --open",
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "review": "npm run lint && npm test",
    "test": "jest src/**",
    "test:watch": "jest src/** --watch",
    "test:watchAll": "jest src/** --watchAll",
    "test:coverage:report": "open coverage/lcov-report/index.html"
  },
  "engines": {
    "node": "16.x"
  },
  "author": "samalexander10",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.27.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "ajv": "^8.17.1",
    "axios": "^1.7.7",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "html-webpack-plugin": "^5.6.3",
    "json-schema-traverse": "^1.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/aria-query": "^5.0.4",
    "ansi-styles": "^5.2.0",
    "aria-query": "^5.3.0",
    "chalk": "^4.1.2",
    "dequal": "^2.0.3",
    "dom-accessibility-api": "^0.5.16",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-watch": "^8.0.0",
    "jest": "^29.7.0",
    "lz-string": "^1.5.0",
    "pretty-format": "^27.5.1",
    "react-is": "^17.0.2",
    "react-test-renderer": "^18.3.1",
    "webpack-dev-server": "^5.1.0",
    "webpack-hot-middleware": "^2.26.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "<rootDir>/__tests__/cssStub.js"
    },
    "setupFiles": [
      "<rootDir>/__tests__/setupTests.js"
    ]
  }
}